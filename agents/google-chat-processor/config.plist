<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.petesbrain.google-chat-processor</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/python3</string>
        <string>/Users/administrator/Documents/PetesBrain/agents/system/google-chat-processor.py</string>
        <string>--days</string>
        <string>7</string>
    </array>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    </dict>

    <key>WorkingDirectory</key>
    <string>/Users/administrator/Documents/PetesBrain</string>

    <key>StartInterval</key>
    <integer>3600</integer>

    <key>StandardOutPath</key>
    <string>/Users/administrator/.petesbrain-google-chat.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/administrator/.petesbrain-google-chat-error.log</string>

    <key>RunAtLoad</key>
    <true/>

<key>ProcessType</key>
<string>Background</string>
</dict>
</plist>

