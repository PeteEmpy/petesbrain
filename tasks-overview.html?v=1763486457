let manualNotes = JSON.parse(localStorage.getItem('taskNotes') || '{}');

function escapeHtml(text) {
    const div = document.createElement('div');
    div.textContent = text;
    return div.innerHTML;
}

function renderClients() {
    const container = document.getElementById('clients-container');
    container.innerHTML = CLIENT_DATA.map(client => `
        <div class="client-section">
            <div class="client-header" onclick="toggleClient('${client.name}')">
                <div class="client-name">${client.display}</div>
                <div class="client-stats">
                    <div class="client-stat">Active: ${client.active_count}</div>
                    ${client.p0_count > 0 ? `<div class="client-stat urgent">\ud83d\udd25 P0: ${client.p0_count}</div>` : ''}
                    <div class="client-stat">Completed: ${client.completed_count}</div>
                </div>
            </div>
            <div class="client-content" id="client-${client.name}">
                ${renderTasks(client)}
                ${renderCompleted(client)}
            </div>
        </div>
    `).join('');
}

function renderTasks(client) {
    let html = '';

    // Parent tasks with children
    if (client.parent_tasks.length > 0) {
        html += '<div class="task-group"><div class="task-group-title">\ud83d\udccb Parent Tasks</div>';
        client.parent_tasks.forEach(parent => {
            const hasNote = manualNotes[parent.id];
            html += `
                <div class="parent-task ${hasNote ? 'has-manual-note' : ''}" onclick="toggleParent(event, '${parent.id}')">
                    <h4>${escapeHtml(parent.title)}</h4>
                    <div class="parent-meta">
                        <span>\ud83c\udfaf ${parent.priority}</span>
                        <span>\ud83d\udcc5 ${parent.due_date || 'Not set'}</span>
                        <span>\ud83d\udcca ${client.child_tasks.filter(c => c.parent_id === parent.id).length} subtasks</span>
                    </div>
                    ${parent.notes ? `<div class="parent-notes">${escapeHtml(parent.notes)}</div>` : ''}
                    <button class="add-note-btn" onclick="event.stopPropagation(); showNoteInput('${parent.id}')">
                        ${hasNote ? 'Edit Note' : 'Add Note'}
                    </button>
                    <div class="manual-note-input" id="note-input-${parent.id}">
                        <textarea placeholder="Add your manual note here..."></textarea>
                        <div class="btn-group">
                            <button class="save-note-btn" onclick="event.stopPropagation(); saveNote('${parent.id}')">Process Note</button>
                            <button class="cancel-note-btn" onclick="event.stopPropagation(); hideNoteInput('${parent.id}')">Cancel</button>
                        </div>
                    </div>
                    <div class="manual-note-display ${hasNote ? 'active' : ''}" id="note-display-${parent.id}">
                        <div class="note-header">\ud83d\udcdd Manual Note:</div>
                        <div class="note-text">${escapeHtml(hasNote || '')}</div>
                        <button onclick="event.stopPropagation(); showNoteInput('${parent.id}')">Edit</button>
                    </div>
                </div>
                <div class="child-tasks" id="children-${parent.id}">
                    ${client.child_tasks.filter(c => c.parent_id === parent.id).map(child => renderTask(child)).join('')}
                </div>
            `;
        });
        html += '</div>';
    }

    // Standalone tasks
    if (client.standalone_tasks.length > 0) {
        html += '<div class="task-group"><div class="task-group-title">\ud83d\udccc Standalone Tasks</div>';
        client.standalone_tasks.forEach(task => {
            html += renderTask(task);
        });
        html += '</div>';
    }

    return html || '<div class="no-tasks">No active tasks</div>';
}

function renderTask(task) {
    const hasNote = manualNotes[task.id];
    return `
        <div class="task priority-${task.priority || 'P2'} ${hasNote ? 'has-manual-note' : ''}" onclick="toggleTask('${task.id}')">
            <div class="task-title">${escapeHtml(task.title)}</div>
            <div class="task-meta">
                <span>\ud83c\udfaf ${task.priority}</span>
                <span>\u23f1\ufe0f ${task.time_estimate_mins || 'N/A'} mins</span>
                <span>\ud83d\udcc5 ${task.due_date || 'Not set'}</span>
            </div>
            ${task.notes ? `<div class="task-notes">${escapeHtml(task.notes)}</div>` : ''}
            <button class="add-note-btn" onclick="event.stopPropagation(); showNoteInput('${task.id}')">
                ${hasNote ? 'Edit Note' : 'Add Note'}
            </button>
            <div class="manual-note-input" id="note-input-${task.id}">
                <textarea placeholder="Add your manual note here..."></textarea>
                <div class="btn-group">
                    <button class="save-note-btn" onclick="event.stopPropagation(); saveNote('${task.id}')">Process Note</button>
                    <button class="cancel-note-btn" onclick="event.stopPropagation(); hideNoteInput('${task.id}')">Cancel</button>
                </div>
            </div>
            <div class="manual-note-display ${hasNote ? 'active' : ''}" id="note-display-${task.id}">
                <div class="note-header">\ud83d\udcdd Manual Note:</div>
                <div class="note-text">${escapeHtml(hasNote || '')}</div>
                <button onclick="event.stopPropagation(); showNoteInput('${task.id}')">Edit</button>
            </div>
        </div>
    `;
}

function renderCompleted(client) {
    if (client.completed_tasks.length === 0) return '';

    return `
        <div class="completed-section">
            <div class="completed-header" onclick="toggleCompleted('${client.name}')">
                <span>\u2705 Completed Tasks (${client.completed_tasks.length})</span>
                <span>\u25bc</span>
            </div>
            <div class="completed-tasks" id="completed-${client.name}">
                ${client.completed_tasks.map(task => `
                    <div class="completed-task">
                        <div class="task-title">${escapeHtml(task.title)}</div>
                        <div class="completed-date">Completed: ${task.completed_at ? new Date(task.completed_at).toLocaleDateString() : 'Unknown'}</div>
                        ${task.notes ? `<div style="margin-top:5px;font-size:11px;color:#777">${escapeHtml(task.notes.substring(0, 150))}${task.notes.length > 150 ? '...' : ''}</div>` : ''}
                    </div>
                `).join('')}
            </div>
        </div>
    `;
}

function toggleClient(name) {
    const content = document.getElementById('client-' + name);
    content.classList.toggle('expanded');
}

function toggleParent(event, id) {
    event.currentTarget.classList.toggle('expanded');
    const children = document.getElementById('children-' + id);
    if (children) children.classList.toggle('expanded');
}

function toggleTask(id) {
    const task = event.currentTarget;
    task.classList.toggle('expanded');
}

function toggleCompleted(clientName) {
    const completed = document.getElementById('completed-' + clientName);
    completed.classList.toggle('expanded');
}

function toggleAll() {
    const contents = document.querySelectorAll('.client-content');
    const anyExpanded = Array.from(contents).some(c => c.classList.contains('expanded'));
    contents.forEach(c => anyExpanded ? c.classList.remove('expanded') : c.classList.add('expanded'));
    document.querySelector('.expand-all').textContent = anyExpanded ? 'Expand All Clients' : 'Collapse All Clients';
}

function showNoteInput(taskId) {
    const input = document.getElementById('note-input-' + taskId);
    const display = document.getElementById('note-display-' + taskId);
    const textarea = input.querySelector('textarea');
    textarea.value = manualNotes[taskId] || '';
    input.classList.add('active');
    if (display) display.classList.remove('active');
    textarea.focus();
}

function hideNoteInput(taskId) {
    const input = document.getElementById('note-input-' + taskId);
    const display = document.getElementById('note-display-' + taskId);
    input.classList.remove('active');
    if (manualNotes[taskId] && display) {
        display.classList.add('active');
    }
}

function saveNote(taskId) {
    const textarea = document.querySelector(`#note-input-${taskId} textarea`);
    const noteText = textarea.value.trim();

    if (noteText) {
        manualNotes[taskId] = noteText;
        const displayText = document.querySelector(`#note-display-${taskId} .note-text`);
        const display = document.getElementById('note-display-' + taskId);
        if (displayText) displayText.textContent = noteText;
        if (display) display.classList.add('active');

        // Add yellow border to task
        const taskElements = document.querySelectorAll(`[onclick*="${taskId}"]`);
        taskElements.forEach(el => el.classList.add('has-manual-note'));

        // Update "Add Note" button text
        const buttons = document.querySelectorAll(`[onclick*="showNoteInput('${taskId}')"]`);
        buttons.forEach(btn => {
            if (btn.classList.contains('add-note-btn')) {
                btn.textContent = 'Edit Note';
            }
        });

        // Find the task context from CLIENT_DATA
        let taskContext = findTaskInClientData(taskId);

        if (taskContext) {
            // Create single-item array with note and task context
            const noteData = [{
                task_id: taskId,
                client: taskContext.client,
                task_title: taskContext.task.title,
                task_type: taskContext.task.type,
                task_priority: taskContext.task.priority,
                task_notes: taskContext.task.notes || '',
                manual_note: noteText,
                timestamp: new Date().toISOString()
            }];

            // Download as JSON file
            const blob = new Blob([JSON.stringify(noteData, null, 2)], { type: 'application/json' });
            const url = URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = url;
            a.download = 'manual-task-notes.json';
            document.body.appendChild(a);
            a.click();
            document.body.removeChild(a);
            URL.revokeObjectURL(url);

            // Copy message to clipboard
            const message = 'Process my task notes';
            navigator.clipboard.writeText(message).then(() => {
                alert(' Note downloaded to Downloads folder as manual-task-notes.json\n\n "Process my task notes" copied to clipboard\n\n Now paste (Cmd+V) into Claude Code to process this note');
            }).catch(err => {
                alert(' Note downloaded to Downloads folder as manual-task-notes.json\n\n Could not copy to clipboard automatically.\n\n Type "Process my task notes" into Claude Code');
            });
        }
    } else {
        delete manualNotes[taskId];
        const taskElements = document.querySelectorAll(`[onclick*="${taskId}"]`);
        taskElements.forEach(el => el.classList.remove('has-manual-note'));

        const buttons = document.querySelectorAll(`[onclick*="showNoteInput('${taskId}')"]`);
        buttons.forEach(btn => {
            if (btn.classList.contains('add-note-btn')) {
                btn.textContent = 'Add Note';
            }
        });
    }

    localStorage.setItem('taskNotes', JSON.stringify(manualNotes));
    hideNoteInput(taskId);
    updateNoteCount();
}

function findTaskInClientData(taskId) {
    // Search through all clients to find the task
    for (const client of CLIENT_DATA) {
        // Check parent tasks
        for (const task of client.parent_tasks || []) {
            if (task.id === taskId) {
                return { client: client.name, task: task };
            }
        }
        // Check child tasks
        for (const task of client.child_tasks || []) {
            if (task.id === taskId) {
                return { client: client.name, task: task };
            }
        }
        // Check standalone tasks
        for (const task of client.standalone_tasks || []) {
            if (task.id === taskId) {
                return { client: client.name, task: task };
            }
        }
        // Check completed tasks
        for (const task of client.completed_tasks || []) {
            if (task.id === taskId) {
                return { client: client.name, task: task };
            }
        }
    }
    return null;
}

function updateNoteCount() {
    const count = Object.keys(manualNotes).length;
    document.getElementById('unsaved-count').textContent = count;
    document.getElementById('save-notes-btn').style.display = count > 0 ? 'inline-block' : 'none';
}

function updateStats() {
    document.getElementById('total-clients').textContent = CLIENT_DATA.length;
    document.getElementById('total-active').textContent = CLIENT_DATA.reduce((sum, c) => sum + c.active_count, 0);
    document.getElementById('total-p0').textContent = CLIENT_DATA.reduce((sum, c) => sum + c.p0_count, 0);
    document.getElementById('total-completed').textContent = CLIENT_DATA.reduce((sum, c) => sum + c.completed_count, 0);
}

function saveAllNotes() {
    if (Object.keys(manualNotes).length === 0) {
        alert('No notes to save.');
        return;
    }

    // Prepare notes with task context
    const notesWithContext = [];
    Object.keys(manualNotes).forEach(taskId => {
        // Find the task in CLIENT_DATA
        let taskContext = null;
        let clientName = null;

        for (const client of CLIENT_DATA) {
            // Check parent tasks
            const parentTask = client.parent_tasks.find(t => t.id === taskId);
            if (parentTask) {
                taskContext = parentTask;
                clientName = client.name;
                break;
            }

            // Check child tasks
            const childTask = client.child_tasks.find(t => t.id === taskId);
            if (childTask) {
                taskContext = childTask;
                clientName = client.name;
                break;
            }

            // Check standalone tasks
            const standaloneTask = client.standalone_tasks.find(t => t.id === taskId);
            if (standaloneTask) {
                taskContext = standaloneTask;
                clientName = client.name;
                break;
            }
        }

        if (taskContext && clientName) {
            notesWithContext.push({
                task_id: taskId,
                client: clientName,
                task_title: taskContext.title,
                task_type: taskContext.type,
                task_priority: taskContext.priority,
                task_notes: taskContext.notes,
                manual_note: manualNotes[taskId],
                parent_id: taskContext.parent_id
            });
        }
    });

    // Try to save to PetesBrain fixed location via Python server
    const dataStr = JSON.stringify(notesWithContext, null, 2);

    fetch('http://localhost:8765/save-notes', {
        method: 'POST',
        headers: { 'Content-Type': 'application/json' },
        body: dataStr
    })
    .then(response => response.json())
    .then(data => {
        if (data.success) {
            // Copy message to clipboard
            const clipboardMessage = `Process my task notes`;
            navigator.clipboard.writeText(clipboardMessage).then(() => {
                alert(` Task notes saved!\n\n${notesWithContext.length} note(s) saved to:\n${data.file}\n\n "Process my task notes" copied to clipboard!\n\nJust paste (Cmd+V) into Claude Code to process.`);
            }).catch(() => {
                alert(` Task notes saved!\n\n${notesWithContext.length} note(s) saved to:\n${data.file}\n\nTell Claude: "Process my task notes"`);
            });
        } else {
            throw new Error(data.error || 'Unknown error');
        }
    })
    .catch(error => {
        // Fallback: Download to browser Downloads folder if server not running
        console.error('Server not available, falling back to download:', error);

        const dataBlob = new Blob([dataStr], {type: 'application/json'});
        const link = document.createElement('a');
        link.href = URL.createObjectURL(dataBlob);
        link.download = 'manual-task-notes.json';
        document.body.appendChild(link);
        link.click();
        document.body.removeChild(link);

        alert(` Task notes server not running!\n\nFallback: Downloaded to your Downloads folder.\n\n To fix this:\n1. Open Terminal\n2. Run: python3 ~/Documents/PetesBrain/shared/scripts/save-task-notes.py\n3. Keep it running in background\n\nThen try "Process All Notes" again.\n\n(For now: Move manual-task-notes.json from Downloads to ~/Documents/PetesBrain/data/state/)`);
    });
}

// Initialize
renderClients();
updateStats();
updateNoteCount();
    </script>
</body>
</html>
