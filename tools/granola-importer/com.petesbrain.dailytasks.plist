<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.petesbrain.dailytasks</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/administrator/Documents/PetesBrain/tools/granola-importer/venv/bin/python3</string>
        <string>/Users/administrator/Documents/PetesBrain/tools/granola-importer/generate_daily_tasks.py</string>
    </array>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>8</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/administrator/Documents/PetesBrain/tools/granola-importer/daily-tasks.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/administrator/Documents/PetesBrain/tools/granola-importer/daily-tasks-error.log</string>

    <key>RunAtLoad</key>
    <false/>

    <key>WorkingDirectory</key>
    <string>/Users/administrator/Documents/PetesBrain/tools/granola-importer</string>
</dict>
</plist>
