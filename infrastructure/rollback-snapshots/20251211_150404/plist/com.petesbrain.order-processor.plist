<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.petesbrain.order-processor</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/python3</string>
        <string>/Users/administrator/Documents/PetesBrain.nosync/personal/orders/process-orders.py</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/administrator/Documents/PetesBrain.nosync/personal/orders</string>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>9</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/Users/administrator/.petesbrain-order-processor.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/administrator/.petesbrain-order-processor.log</string>

    <key>ProcessType</key>
    <string>Background</string>
</dict>
</plist>
