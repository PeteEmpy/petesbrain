<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.petesbrain.facebook-specs-processor</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/python3</string>
        <string>/Users/administrator/Documents/PetesBrain.nosync/agents/facebook-specs-processor/facebook-specs-processor.py</string>
    </array>

    <key>EnvironmentVariables</key>
    <dict>
    </dict>

    <key>WorkingDirectory</key>
    <string>/Users/administrator/Documents/PetesBrain.nosync</string>

    <key>StartInterval</key>
    <integer>7200</integer>

    <key>StandardOutPath</key>
    <string>/Users/administrator/.petesbrain-facebook-specs-processor.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/administrator/.petesbrain-facebook-specs-processor-error.log</string>

    <key>RunAtLoad</key>
    <true/>

<key>ProcessType</key>
<string>Background</string>
</dict>
</plist>

